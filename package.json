{
    "name": "maturita",
    "version": "1.0.0",
    "description": "A fictional company website for my maturita work",
    "homepage": "https://davidstraka2.github.io/maturita",
    "bugs": "https://github.com/davidstraka2/maturita/issues",
    "license": "MIT",
    "author": "David Straka (https://github.com/davidstraka2)",
    "main": "index.html",
    "repository": {
        "type": "git",
        "url": "https://github.com/davidstraka2/maturita"
    },
    "scripts": {
        "autoprefixer": "postcss -u autoprefixer -r css/*",
        "scss": "node-sass --output-style compressed -o css scss",
        "start": "index.html",
        "watch-scss": "onchange \"scss/*.scss\" -- npm run scss"
    },
    "devDependencies": {
        "autoprefixer": "^7.1.6",
        "eslint": "^4.11.0",
        "eslint-config-airbnb-base": "^12.1.0",
        "eslint-plugin-import": "^2.8.0",
        "node-sass": "^4.6.1",
        "onchange": "^3.2.1",
        "postcss-cli": "^4.1.1"
    },
    "private": true,
    "browserslist": [
        "last 2 Chrome versions",
        "last 2 ChromeAndroid versions",
        "last 2 Edge versions",
        "last 2 Firefox versions",
        "last 2 Opera versions"
    ],
    "eslintConfig": {
        "extends": "airbnb-base"
    }
}
